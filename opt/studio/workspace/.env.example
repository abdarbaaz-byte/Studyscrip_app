# Firebase Configuration
# Copy these variables to a .env file and fill them with your project's credentials.
NEXT_PUBLIC_FIREBASE_API_KEY="YOUR_API_KEY"
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN="YOUR_PROJECT_ID.firebaseapp.com"
NEXT_PUBLIC_FIREBASE_PROJECT_ID="YOUR_PROJECT_ID"
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET="YOUR_PROJECT_ID.appspot.com"
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID="YOUR_MESSAGING_SENDER_ID"
NEXT_PUBLIC_FIREBASE_APP_ID="YOUR_APP_ID"
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID="YOUR_MEASUREMENT_ID"

# Razorpay Configuration (Optional)
# Used for processing online payments.
RAZORPAY_KEY_ID="YOUR_RAZORPAY_KEY_ID"
RAZORPAY_KEY_SECRET="YOUR_RAZORPAY_KEY_SECRET"

# OneSignal Configuration (Optional)
# Used for push notifications.
NEXT_PUBLIC_ONESIGNAL_APP_ID="YOUR_ONESIGNAL_APP_ID"

# UPI Configuration (Optional)
# Your UPI ID for manual payments.
NEXT_PUBLIC_UPI_ID="your-upi-id@okhdfcbank"
